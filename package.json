{
  "name": "textlint-plugin-markdown",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textlint/textlint-plugin-markdown.git"
  },
  "author": "azu",
  "email": "azuciao@gmail.com",
  "homepage": "https://github.com/textlint/textlint-plugin-markdown",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/textlint/textlint-plugin-markdown/issues"
  },
  "version": "1.1.0",
  "description": "Markdown support for textlint.",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir lib --source-maps",
    "watch": "babel src --out-dir lib --watch --source-maps",
    "prepublish": "npm run --if-present build",
    "test": "mocha"
  },
  "keywords": [
    "textlint",
    "plugin",
    "markdown"
  ],
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.9.0",
    "mocha": "^2.3.3",
    "power-assert": "^1.4.1",
    "textlint": "^5.3.0",
    "textlint-rule-no-todo": "^1.0.3"
  },
  "dependencies": {
    "markdown-to-ast": "^3.4.0"
  }
}
